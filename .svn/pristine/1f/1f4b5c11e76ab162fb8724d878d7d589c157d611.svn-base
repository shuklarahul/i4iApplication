    $(document).ready(function () {
        var $filter = $('#filter'),
            $paragraphs = $('#content table tbody tr');
        $("#filter").keyup(function () {
            var value = $filter.val(),
            matcher = new RegExp('\\b' + value, 'i');
            $paragraphs.each(function () {
                var $p = $(this),
                    content = $p.text();
                if (matcher.test(content)) {
                    $p.show();
                } else {
                    $p.hide();
                }
            });
        });
    });
    function ShowDataAgain() {
        document.getElementById('tblData').innerHTML = "";
        $.ajax({
            url: "/WorldRef/ListExcelProjectsAdminJason",
            type: 'POST',
            data: JSON.stringify({}),
            contentType: 'application/json; charset=utf-8',
            success: function (response) {
                var Result = "";
                $.each(response, function (id, model) {
                    //  alert('Method called');
                    //   alert(model.Createdon);
                    var date;
                    if (model.Createdon != null) {
                        date = new Date(parseInt(model.Createdon.substr(6))).toLocaleDateString();
                        //  var formatdate = date.format("dd-mm-yyyy");
                    }
                    Result += '<tr><td>' + model.OrganizationName + '</td>';
                    Result += '<td>' + model.CustomerName + '</td>';
                    Result += '<td>' + model.ProjectName + '</td>';
                    Result += '<td>' + model.Type + '</td>';
                    Result += '<td>' + model.Year + '</td>';
                    Result += '<td>' + model.Country + '</td>';
                    //    Result += '<td>' + model.EmailId + '</td>';
                    Result += '<td>' + date + '</td>';
                    Result += '<td><input type="checkbox" name="ids" value="' + model.Id + '"/>';
                    Result += '<input type="button" id="' + model.Id + '" name="Disapprove" value="Disapprove" onclick="showmodalpopup(this.id);"/></td></tr>';
                });
                Result += '</table>';
                $("#tblData").html(Result);
            },
            error: function () {
                alert("Failed! Please try again.");
            }
        });
    }
    function GetTotalUnPublishedReview(id) {
        $("#loading").show();
        $.ajax({
            url: "/WorldRef/TotalUnpblishedReview",
            type: 'POST',
            data: JSON.stringify({ "id": id }),
            contentType: 'application/json; charset=utf-8',
            success: function (data) {

                if (data == "") {
                    var Result = "<div style='width:480px;'><h4 style='margin:0 0 7px 0;'>Unpublished Testimonial</h4>";
                    Result += " <div class='noRecord fW'>No Testimonial Available. </div></div>";
                    var foo = $("#ReviewList").html(Result);
                    $.fancybox(foo);
                    $("#loading").hide();

                }

                else {
                    var Result = "<div style='width:480px;'><h4 style='margin:0 0 7px 0;'>Unpublished Testimonial</h4>";
                    for (i = 0; i < data.length; i++) {
                        if (i % 2 == 0) {
                            Result += '<div class="noreview">';
                        }
                        else {
                            Result += '<li class="mbColor1">';
                        }

                        Result += "<span style='font-weight:bold;color: #636262;'></span>" + data[i].Review + "</li>";

                    }

                    var foo = $("#ReviewList").html(Result);
                    $.fancybox(foo);
                    $("#loading").hide();
                }
            },
            error: function () {
                $("#loading").hide();
                alert("Failed! Please try again.");
            }
        });

    }
