@font-face {
	font-family: 'icomoon';
	src:url('../font_icon/icomoon.eot?-n0uotc');
	src:url('../font_icon/icomoon.eot?#iefix-n0uotc') format('embedded-opentype'),
		url('../font_icon/icomoon.woff?-n0uotc') format('woff'),
		url('../font_icon/icomoon.ttf?-n0uotc') format('truetype'),
		url('../font_icon/icomoon.svg?-n0uotc#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}
a{
text-decoration:none;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
   
	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

