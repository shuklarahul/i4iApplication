                -moz-border-right-colors: none;
                -moz-border-top-colors: none;
                background-color: #FFFFFF;
                border-color: #DDDDDD;
                border-image: none;
                border-style: solid;
                border-width: 1px 1px 1px 0;
                float: left;
                line-height: 20px;
                padding: 4px 12px;
                text-decoration: none;
            }

            .pagination ul > li > a, .pagination ul > li > span {
                color: #000;
            }

