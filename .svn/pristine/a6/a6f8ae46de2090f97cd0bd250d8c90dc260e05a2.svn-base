body {
    font-size: 12px;
    font-family: "Open Sans", Arial, sans-serif;
    line-height: 16px;
    min-height: 100%;
    background-color: #F4F4F4;
    width: 100%;
}

* {
    padding: 0;
    margin: 0;
    zoom: 1;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
}

div {
    box-sizing: border-box;
    -moz-box-sizing: border-box;
}

#navmenuicon {
    position: absolute;
    top: 5px;
    right: 5px;
    z-index: 100;
    float: right;
    width: 50%;
}

    #navmenuicon:hover #popmenu {
        display: block;
    }

#menuicon {
    position: absolute;
    top: 1px;
    right: 2px;
}

#navmenuicon #popmenu {
    display: none;
    margin-top: 24px;
    padding: 9px 20px 9px 20px;
    border: 1px solid #d9d9d9;
    border-radius: 4px;
    background-color: #fff;
}

    #navmenuicon #popmenu a {
        line-height: 160%;
        font-size: 110%;
    }

#searchrow {
    height: 47px;
}

#navmenuicon ul {
    display: block;
    margin-top: 6px;
    float: right;
}

    #navmenuicon ul li {
        list-style: none;
        margin-right: 10px;
        float: left;
    }

        #navmenuicon ul li a {
            text-decoration: none;
            color: rgb(101, 99, 99);
        }

            #navmenuicon ul li a:hover {
                color: #225968;
            }

#searchrow {
    width: 100%;
    background-color: #f0f0f0;
    position: relative;
}

.navbar {
    background-color: #FAFAFA;
    background-image: linear-gradient(to bottom, #FFFFFF, #F2F2F2);
    background-repeat: repeat-x;
    border: 1px solid #D4D4D4;
    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.067);
    min-height: 40px;
    padding-left: 20px;
    padding-right: 20px;
}

#menu_logo {
    background-image: url(../img/menu-logo.png);
    background-repeat: no-repeat;
    width: 105px;
    height: 25px;
    float: left;
    margin-top: 11px;
}

.wrapper_div_form_search {
    margin-left: 7%;
    width: 33%;
    float: left;
}

.div_form_search {
    padding-top: 7px;
    width: 100%;
    position: relative;
    margin-left: 0;
}

form {
    /* text-align: center; */
    position: relative;
}

#search_btn {
    position: absolute;
    top: 12px;
    right: 0px;
    width: 38px;
    height: 27px;
    border: 0;
    margin: 0px;
    padding: 0px;
    cursor: pointer;
    background: #D0D0D0;
    background-image: url("../img/s3.png");
    background-repeat: no-repeat;
    background-size: 18px 18px;
    background-position: 9px 5px;
}

.typeahead-widget {
    position: absolute;
    left: 0;
    top: 0;
    padding-left: 2px;
    padding-top: 6px;
    z-index: 1000;
    overflow: hidden;
    white-space: nowrap;
    pointer-events: none;
}

#q {
    display: block;
    color: #000;
    border: solid #ccc 1px;
    margin: 0;
    outline: 0;
}

input.search_field {
    min-width: 200px;
    width: 100%;
    height: 28px;
    outline: 0;
}

input {
    padding: 4px;
    /* border: solid 1px #cccccc;
border-radius: 4px; */
}

.white {
    font-size: 12pt;
    opacity: 0;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter: alpha(opacity=0);
}

.grey {
    color: #CCC;
    overflow: hidden;
    font-size: 12pt;
}

#search_btn {
    top: 0px;
}

#searchmenu {
    border-bottom: 1px solid #f0f0f0;
